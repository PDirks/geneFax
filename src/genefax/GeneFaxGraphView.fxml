<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" fx:id="content" prefHeight="538.0" prefWidth="875.0" style="-fx-background-color: #2d2e33;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="genefax.GeneFaxUIController">
   <children>
      <MenuBar layoutY="-2.0" prefHeight="32.0" prefWidth="875.0" style="-fx-background-color: whitesmoke;">
         <effect>
            <InnerShadow />
         </effect>
         <menus>
            <Menu mnemonicParsing="false" text="Home">
              <items>
                <MenuItem fx:id="triggerHomeViewMenu" mnemonicParsing="false" onAction="#goToHome" text="Home Page" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Close" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" onAction="#load" text="About">
               <items>
                  <MenuItem fx:id="triggerAboutBtn" mnemonicParsing="false" onAction="#about" text="About" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane layoutX="4.0" layoutY="35.0" prefHeight="489.0" prefWidth="875.0">
         <children>
            <Pane fx:id="graph" layoutX="1.0" layoutY="20.0" prefHeight="463.0" prefWidth="865.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
